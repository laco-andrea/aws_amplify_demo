      "service": "Cognito",
      "serviceType": "managed"
    }
  },
  "function": {
    "hello": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "parameters": {
    "AMPLIFY_function_hello_deploymentBucketName": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "hello"
        }
